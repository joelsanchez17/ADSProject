0020e1b3            OR x3, x1, x2       // x3 = x1 OR x2  = 0101 OR 1010 = 1111 = 15
0020f1b3            AND x3, x1, x2      // x3 = x1 AND x3 = 0101 AND 1010 = 0000
0020a1b3            SLT x3, x1, x2      // x3 ( 5 < 10 ) = 1
0020b1b3            SLTU x3, x1, x2     // x3 = 1 ( 5 < 10 ) = 1 (unsigned)
